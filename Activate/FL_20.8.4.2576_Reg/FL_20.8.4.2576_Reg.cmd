@echo Off
set rParam=FL Studio
set rKey=HKCU\SOFTWARE\Image-Line\Shared\Paths
Reg Query "%rKey%" /v "%rParam%" 2>nul >nul && goto 1
set rKey=HKLM\SOFTWARE\Image-Line\Shared\Paths
Reg Query "%rKey%" /v "%rParam%" 2>nul >nul && goto 1
echo FL Studio install path not found.& echo Install FL Studio and run FL_Reg.cmd file.& goto end
:1
for /f "Tokens=3*" %%a In ('Reg Query "%rKey%" /v "%rParam%"') Do call set FLPath="%%~dpbSystem\Internet\Default news.rss"
del /q /f %FLPath% 2>nul & rmdir /s /q %FLPath% 2>nul
copy /y "%cd%\Default news.20.8.4.2576.rss" %FLPath% >nul 2>nul

set rParam=Shared data
set rKey=HKCU\SOFTWARE\Image-Line\Shared\Paths
Reg Query "%rKey%" /v "%rParam%" 2>nul >nul && goto 2
set rKey=HKLM\SOFTWARE\Image-Line\Shared\Paths
Reg Query "%rKey%" /v "%rParam%" 2>nul >nul && goto 2
echo FL Studio User data folder path not found.& echo Start and close FL Studio and run FL_Reg.cmd file.&echo. & goto 3
:2
for /f "Tokens=3*" %%a In ('Reg Query "%rKey%" /v "%rParam%"') Do call set FLPath="%%bFL Studio\Settings\Internet\Default news.rss"
del /q /f %FLPath% 2>nul & rmdir /s /q %FLPath% 2>nul
:3
Reg Add "HKCU\SOFTWARE\Image-Line\Registrations\FL Studio 20.8" /v "ILRegKey" /t "REG_SZ" /d "" /f 2>nul >nul
if Errorlevel 1 echo Failed to add ILRegKey. & goto end
echo Done! FL Studio is activated.
:end
echo.& echo Close this window or press any key to exit . . .
pause >nul & exit
